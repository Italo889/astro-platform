{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/app.js",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "prisma generate && tsc",
    "build:safe": "tsc --noEmitOnError false",
    "postbuild": "prisma generate",
    "heroku-postbuild": "prisma generate && npm run build:safe",
    "start": "node dist/app.js",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "env:dev": "node setup-env.js development",
    "env:prod": "node setup-env.js production",
    "setup": "npm run env:dev && npm run generate"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.2.0",
    "@prisma/client": "^6.13.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bullmq": "^5.56.10",
    "dotenv": "^17.2.1",
    "ephemeris": "^2.2.0",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "readline": "^1.3.0",
    "swisseph": "^0.5.17",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/pg": "^8.15.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0"
  }
}
